{
  "name": "docz-plugin-storybook",
  "version": "0.0.0",
  "description": "Docz plugin that makes migrating from Storybook a breeze.",
  "author": "Hydrate <dev@hydrate.io> (https://hydrate.io)",
  "repository": "hydrateio/docz-plugin-storybook",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "yarn": ">=1.5.0"
  },
  "keywords": [
    "docz",
    "storybook",
    "react",
    "reactjs",
    "plugin",
    "mdx"
  ],
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "babel src -d dist",
    "prestart": "yarn run clean",
    "start": "babel src -w -d dist",
    "dev": "yarn run start",
    "clean": "del dist",
    "lint": "eslint .",
    "test": "yarn run lint && jest"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "docz-core": "^0.12.5",
    "globby": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "del-cli": "^1.1.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-test-renderer": "^16.6.0"
  }
}
