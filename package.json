{
  "name": "docz-plugin-storybook",
  "version": "0.3.1",
  "description": "Docz plugin that makes migrating from Storybook a breeze.",
  "author": "Hydrate <dev@hydrate.io> (https://hydrate.io)",
  "repository": "hydrateio/docz-plugin-storybook",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "docz",
    "storybook",
    "react",
    "reactjs",
    "plugin",
    "mdx"
  ],
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "babel src -s -d dist",
    "build:examples": "cd examples/basic && yarn && yarn docz:build",
    "prestart": "yarn run clean",
    "start": "babel src -w -s -d dist",
    "dev": "yarn run start",
    "clean": "del dist",
    "lint": "eslint .",
    "pretest": "yarn build",
    "test": "yarn run lint && jest && yarn build:examples"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@sindresorhus/slugify": "^0.6.0",
    "detect-port": "^1.2.3",
    "fs-extra": "^7.0.0",
    "koa-static": "^5.0.0",
    "p-retry": "^3.0.1",
    "puppeteer": "^1.9.0",
    "react": "^16.6.1",
    "read-pkg-up": "^4.0.0",
    "resolve": "^1.8.1",
    "semver": "^5.6.0",
    "serve-handler": "^5.0.5",
    "tempy": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addons": "^4.0.4",
    "@storybook/channel-postmessage": "^4.0.4",
    "@storybook/react": "4.0.4",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "del-cli": "^1.1.0",
    "docz-core": "0.12.16",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react-test-renderer": "^16.6.0"
  },
  "peerDependencies": {
    "@storybook/react": "^3 || ^4",
    "docz-core": ">= 0.12.10"
  },
  "jest": {
    "bail": true
  }
}
